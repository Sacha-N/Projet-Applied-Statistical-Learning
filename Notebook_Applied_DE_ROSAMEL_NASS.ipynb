    "print(grille_densite.columns.tolist())\n",
    "\n",
    "cols_grille = [\n",
    "    \"CODGEO\",\n",
    "    \"DENS\",# De 1 à 7\n",
    "    \"PMUN17\" #Pop° municipale 2017\n",
