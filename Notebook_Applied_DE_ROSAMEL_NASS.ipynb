    "grille_reduite = grille_densite[cols_grille]\n",
    "\n",
    "# On corrige les formats\n",
    "df[\"COMM\"] = df[\"COMM\"].astype(str).str.zfill(5)\n",
    "grille_densite[\"CODGEO\"] = grille_densite[\"CODGEO\"].astype(str).str.zfill(5)\n",
